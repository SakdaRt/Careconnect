{
  "name": "careconnect-backend",
  "version": "1.0.0",
  "description": "Careconnect Backend API Server",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --coverage --passWithNoTests",
    "test:watch": "jest --watch",
    "test:integration": "jest tests/integration --verbose",
    "test:smoke": "jest tests/integration --testNamePattern='Tier-0' --verbose",
    "lint": "eslint src/**/*.js",
    "migrate": "node scripts/migrate.js migrate",
    "migrate:status": "node scripts/migrate.js status",
    "migrate:bootstrap": "node scripts/migrate.js bootstrap"
  },
  "keywords": [
    "careconnect",
    "healthcare",
    "backend"
  ],
  "author": "Careconnect Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.5",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "date-fns": "^3.0.6",
    "dotenv": "^16.3.1",
    "exif-parser": "^0.1.12",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "google-auth-library": "^9.15.0",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "pg": "^8.11.3",
    "sharp": "^0.33.1",
    "socket.io": "^4.6.1",
    "twilio": "^4.20.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
